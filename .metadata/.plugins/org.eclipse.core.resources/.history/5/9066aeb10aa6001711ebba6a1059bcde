package Main;

public class Car extends Vehicle {
	private final double CAR_PRICE_PER_HOUR = 25.00;
	private VehicleBehavior vehicleBehavior;
	
	
	public Car(){
		vehicleBehavior = new SetAsCar();
	}
	@Override
	public void display() {
		System.out.println("A car costs " + CAR_PRICE_PER_HOUR + "/hr");
	}
	



}
