package Main;

public abstract class Vehicle {
	private VehicleBehavior vehicleBehavior;
	
	
	

//	private final double ALL_DAY_FLAT_FEE = 75.00;
	// private final double TIME_LIMIT_EXCEEDED_PER_HOUR = 35.00;

	public Vehicle(VehicleBehavior vehicleBehavior) {
		super();
		this.vehicleBehavior = vehicleBehavior;
	}



	public void setVehicleBehavior(VehicleBehavior vehicleBehavior){
		this.vehicleBehavior = vehicleBehavior;
	}



	public abstract void display();
}
