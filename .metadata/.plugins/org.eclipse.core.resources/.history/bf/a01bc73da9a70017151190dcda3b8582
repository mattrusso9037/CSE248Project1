package Main;

public class Car extends Vehicle {
	private final double CAR_PRICE_PER_HOUR = 25.00;
	private PriceBehavior priceBehavior;
	private SlotBehavior slotBehavior;
	private double price;
	
	public Car(){
		priceBehavior = new CarPrice();
		slotBehavior = new CarSlotNumber();
	}

	
	public double calculate(int hours, Car car){
		
		price = hours * CAR_PRICE_PER_HOUR;
		return price;
		
	}

	@Override
	public void display() {
		System.out.println("Parking costs: $"+ price  );
		
	}

}
